    "moment": "^2.29.4",
    "pinia": "^2.0.36",
    "prismjs": "^1.29.0",
    "vue": "^3.3.2",
    "vue-echarts": "^6.6.1",
    "vue-router": "^4.2.0",
    "vue3-apexcharts": "^1.4.4",
    "vuex": "^4.1.0",
