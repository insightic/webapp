    "@types/prismjs": "^1.26.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "~5.2.2",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.19"
