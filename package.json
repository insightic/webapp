    "@tabler/core": "^1.0.0-beta20",
    "apexcharts": "^3.44.0",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "echarts": "^5.4.3",
    "moment": "^2.29.4",
    "pinia": "^2.1.7",
    "prismjs": "^1.29.0",
    "vue": "^3.3.4",
    "vue-echarts": "^6.6.1",
    "vue-router": "^4.2.5",
    "vue3-apexcharts": "^1.4.4",
    "vuex": "^4.1.0",
    "vuex-persistedstate": "^4.1.0"
